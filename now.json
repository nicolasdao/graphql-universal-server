{
	"environment": {
		"active": "default",
		"default": {
			"hostingType": "localhost",
			"env": "dev"
		},
		"staging": {
			"hostingType": "gcp",
			"env": "staging",
			"gcp": {
				"functionName": "webfunc-app",
				"memory": 128
			}
		},
		"uat": {
			"hostingType": "aws",
			"env": "uat",
			"aws": {
				"memory": 128,
				"region": "ap-southeast-2"
			}
		},
		"production": {
			"hostingType": "now",
			"env": "prod",
			"scripts": {
				"start": "NODE_ENV=production node index.js"
			},
			"alias": "webfunc-app"
		}
	}
}